{
  "name": "catala-website",
  "version": "1.0.0",
  "repository": "https://github.com/CatalaLang/catala-website",
  "scripts": {
    "build:styles": "tailwind build -i src/css/index.css -o src/css/tailwind.css -c ./src/css/tailwind.config.js",
    "build:styles:watch": "tailwind build -i src/css/index.css -o src/css/tailwind.css -c ./src/css/tailwind.config.js -w",
    "build": "bsb -make-world",
    "start": "bsb -make-world -w -ws _ ",
    "clean": "bsb -clean-world",
    "webpack": "webpack -w",
    "webpack:production": "NODE_ENV=production webpack",
    "server": "webpack serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "BuckleScript",
    "ReasonReact",
    "reason-react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@greenlabs/res-tailwindcss": "^0.1.7",
    "decco": "^1.5.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "reason-react": "^0.9.1",
    "refmt": "^1.13.7-1"
  },
  "devDependencies": {
    "@dylanirlbeck/tailwind-ppx": "^0.7.9",
    "ansi_up": "^4.0.4",
    "autoprefixer": "^10.2.4",
    "browser-builtins": "^3.2.0",
    "browserfs": "^1.4.3",
    "bs-platform": "^8.4.2",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^3.6.0",
    "favicons-webpack-plugin": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "parcel-bundler": "^1.3.1",
    "raw-loader": "^4.0.2",
    "style-loader": "^1.3.0",
    "tailwindcss": "^2.0.2",
    "webpack": "^4.0.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
