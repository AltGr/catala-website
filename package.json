{
  "name": "catala-website",
  "version": "1.0.0",
  "repository": "https://github.com/CatalaLang/catala-website",
  "scripts": {
    "build:styles": "tailwind build -i src/css/index.css -o src/css/tailwind.css -c ./src/css/tailwind.config.js",
    "build:styles:watch": "tailwind build -i src/css/index.css -o src/css/tailwind.css -c ./src/css/tailwind.config.js -w",
    "build": "bsb -make-world",
    "start": "bsb -make-world -w -ws _ ",
    "clean": "bsb -clean-world",
    "webpack": "webpack -w",
    "webpack:production": "NODE_ENV=production webpack",
    "server": "webpack serve",
    "eslint": "npx eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "BuckleScript",
    "ReasonReact",
    "reason-react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@greenlabs/res-tailwindcss": "^0.1.8",
    "constants-browserify": "^1.0.0",
    "decco": "^1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reason-react": "^0.9.1",
    "refmt": "^1.13.7-1",
    "rescript-hash": "^1.5.0"
  },
  "devDependencies": {
    "ansi_up": "^5.1.0",
    "autoprefixer": "^10.4.8",
    "browser-builtins": "^3.3.1",
    "browserfs": "^1.4.3",
    "bs-platform": "^9.0.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.21.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "favicons-webpack-plugin": "^5.0.2",
    "file-loader": "^6.2.0",
    "html-loader": "^4.1.0",
    "html-webpack-plugin": "^5.5.0",
    "parcel-bundler": "^1.3.1",
    "raw-loader": "^4.0.2",
    "style-loader": "^3.3.1",
    "tailwindcss": "^2.2.19",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  }
}
